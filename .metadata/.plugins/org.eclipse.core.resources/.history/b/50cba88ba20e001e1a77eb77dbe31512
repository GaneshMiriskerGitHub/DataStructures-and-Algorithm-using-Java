package package8_BackTracking;

public class A3_Knights_Tour {
	
	public static boolean solveKT() {
		int[][] sol = new int[8][8];
		
		for(int i=0;i<8;i++) {
			for(int j=0;j<8;j++) {
				sol[i][j] = -1;
			}
		}
		
		int xMoves = {2,1,-1,-2,-1, 2, 1,-2};
		int yMoves = {1,2,-2,-1, 2,-1,-2, 1};
		
		sol[0][0] = 1;
	}

	public static void main(String[] args) {
		
		int N = 8;
		
		solveKT();

	}

}
