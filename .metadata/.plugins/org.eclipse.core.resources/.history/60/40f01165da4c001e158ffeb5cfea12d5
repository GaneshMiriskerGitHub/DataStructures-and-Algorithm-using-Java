package packagee18_Graphs;

public class PP19_Dijoint_Set {
	
	public static int findParent(int n, int[] parent) {
		if(n == parent[n]) {
			return n;
		}
		
		findParent(parent[n], parent);
	}

	public static void main(String[] args) {
	
		int n = 7;
		
		int[] parent = new int[n];
		for(int i=0;i<n;i++) {
			parent[i] = i;
		}
		int[] rank = new int[n];
		

	}

}
