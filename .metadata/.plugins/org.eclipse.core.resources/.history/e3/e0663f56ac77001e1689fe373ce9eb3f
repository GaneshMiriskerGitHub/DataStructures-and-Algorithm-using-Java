public class PP16_Tug_of_war { // DP
	
	static int min_dist;
	
	public static void findMinimumInTugOfWar(int[] arr) {
		
		int sum = Arrays.stream(arr).sum();
		
		
		
	}

	public static void main(String[] args) {

		int arr[] = { 23, 45, -34, 12, 0, 98, -99, 4, 189, -1, 4 };
		
