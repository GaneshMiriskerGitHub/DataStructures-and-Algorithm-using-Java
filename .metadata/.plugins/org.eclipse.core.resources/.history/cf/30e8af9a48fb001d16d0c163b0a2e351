package packagee19_dynamic_programming;

public class P1_Fibonacci {
	
	// 0 1 1 2 3
	
	public static int fib(int n) {
		
		if (n <= 1)
            return n;
        return fib(n - 1) + fib(n - 2);
	}

	public static void main(String[] args) {
		
		int n = 5;
		
		System.out.println(fib(n));

	}

}
