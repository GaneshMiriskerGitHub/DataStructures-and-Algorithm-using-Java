import org.springframework.web.bind.annotation.RestController;

@RestController
public class EmployeeController {
	
	@GetMapping("/getAllEmployees")
	public ResponseEntity<?> getAllEmployees() {
		return new ResponseEntity<>(null, HttpStatus.OK);
	}

}
